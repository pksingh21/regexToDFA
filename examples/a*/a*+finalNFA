digraph {
	node [shape=circle]
	3 [label="start state" color=green shape=doublecircle]
	4 [label="end state" color=red shape=doublecircle]
	1 -> 1 [label=a]
	1 -> 2 [label=a]
	1 -> 4 [label=a]
	2 -> 1 [label=a]
	2 -> 2 [label=a]
	2 -> 4 [label=a]
	3 -> 1 [label=a]
	3 -> 2 [label=a]
	3 -> 4 [label=a]
	2 [label="end state" color=red shape=doublecircle]
	3 [label="start + end state" color=violet shape=doublecircle]
	4 [label="end state" color=red shape=doublecircle]
}
